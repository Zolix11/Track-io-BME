<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.DetailedMeasurementsFragment"
    android:background="@drawable/background">


    <ImageView
        android:id="@+id/daily_activities_white_box1"
        android:layout_width="328dp"
        android:layout_height="336dp"
        android:src="@drawable/white_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.217" />

    <TextView
        android:id="@+id/detailed_title"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/trebucbd"
        android:text="Body Measurements"
        android:textColor="@color/primary"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box1"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/daily_activities_white_box1"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.121" />

    <ImageView
        android:id="@+id/detailed_measurements_blue_box"
        android:layout_width="254dp"
        android:layout_height="40dp"
        android:src="@drawable/blue_background"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box1"
        app:layout_constraintStart_toStartOf="@+id/daily_activities_white_box1"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.56" />

    <ImageView
        android:id="@+id/detailed_measurements_blue_box4"
        android:layout_width="254dp"
        android:layout_height="40dp"
        android:src="@drawable/blue_background"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box1"
        app:layout_constraintStart_toStartOf="@+id/daily_activities_white_box1"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.31" />

    <ImageView
        android:id="@+id/detailed_measurements_blue_box2"
        android:layout_width="254dp"
        android:layout_height="40dp"
        android:layout_marginEnd="18dp"
        android:src="@drawable/blue_background"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box1"
        app:layout_constraintHorizontal_bias="0.66"
        app:layout_constraintStart_toStartOf="@+id/daily_activities_white_box1"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.79" />

    <TextView
        android:id="@+id/detailed_meas_height"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/trebucbd"
        android:text="Height"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/detailed_measurements_blue_box"
        app:layout_constraintEnd_toEndOf="@+id/detailed_measurements_blue_box"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="@+id/detailed_measurements_blue_box"
        app:layout_constraintTop_toTopOf="@+id/detailed_measurements_blue_box"
        app:layout_constraintVertical_bias="0.454" />

    <TextView
        android:id="@+id/detailed_meas_weight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/trebucbd"
        android:text="Weight"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/detailed_measurements_blue_box4"
        app:layout_constraintEnd_toEndOf="@+id/detailed_measurements_blue_box4"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="@+id/detailed_measurements_blue_box4"
        app:layout_constraintTop_toTopOf="@+id/detailed_measurements_blue_box4"
        app:layout_constraintVertical_bias="0.545" />

    <TextView
        android:id="@+id/detailed_meas_bmi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/trebucbd"
        android:text="Body Mass Index"
        android:textColor="@color/white"

        app:layout_constraintBottom_toBottomOf="@+id/detailed_measurements_blue_box2"
        app:layout_constraintEnd_toEndOf="@+id/detailed_measurements_blue_box2"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintStart_toStartOf="@+id/detailed_measurements_blue_box2"
        app:layout_constraintTop_toTopOf="@+id/detailed_measurements_blue_box2"
        app:layout_constraintVertical_bias="0.636" />

    <Button
        android:id="@+id/new_details"
        android:layout_width="276dp"
        android:layout_height="50dp"
        android:text="Add new data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.671" />

    <TextView
        android:id="@+id/detailed_meas_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DATE"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box1"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box1"
        app:layout_constraintHorizontal_bias="0.617"
        app:layout_constraintStart_toEndOf="@+id/detailed_title"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box1"
        app:layout_constraintVertical_bias="0.129" />

    <ImageView
        android:id="@+id/daily_activities_white_box4"
        android:layout_width="318dp"
        android:layout_height="115dp"
        android:src="@drawable/white_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GRAPH OF WEIGHT"
        app:layout_constraintBottom_toBottomOf="@+id/daily_activities_white_box4"
        app:layout_constraintEnd_toEndOf="@+id/daily_activities_white_box4"
        app:layout_constraintStart_toStartOf="@+id/daily_activities_white_box4"
        app:layout_constraintTop_toTopOf="@+id/daily_activities_white_box4" />




</androidx.constraintlayout.widget.ConstraintLayout>